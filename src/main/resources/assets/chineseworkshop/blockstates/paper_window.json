{
    "multipart": [
        {   "apply": { "model": "chineseworkshop:block/paper_window_post" }},
        {   "when": { "north": true },
            "apply": { "model": "chineseworkshop:block/paper_window_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "chineseworkshop:block/paper_window_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "chineseworkshop:block/paper_window_side_alt" }
        },
        {   "when": { "west": true },
            "apply": { "model": "chineseworkshop:block/paper_window_side_alt", "y": 90 }
        },
        {   "when": { "north": false },
            "apply": { "model": "chineseworkshop:block/paper_window_noside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "chineseworkshop:block/paper_window_noside_alt" }
        },
        {   "when": { "south": false },
            "apply": { "model": "chineseworkshop:block/paper_window_noside_alt", "y": 90 }
        },
        {   "when": { "west": false },
            "apply": { "model": "chineseworkshop:block/paper_window_noside", "y": 270 }
        }
    ]
}
